<?xml version="1.0"?>
<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/" xmlns:env="http://www.sitecore.net/xmlconfig/env/">
  <sitecore>
	  <!--

Purpose: This include file adds a new site definition
-->
	<sites env:require="ProdCD">
		<site patch:before="site[@name='website']" 
		      name="examplesite"
		      hostName="www.examplesite.com"
		      targetHostName="www.examplesite.com"
		      scheme="http"
		      language="en"
		      virtualFolder="/"
		      physicalFolder="/"
		      rootPath="/sitecore/content/Example Tenant/Example Site"
		      startItem="/home"
		      database="web"
		      domain="extranet"
		      cacheHtml="true"
		      htmlCacheSize="200MB"
		      registryCacheSize="0"
		      viewStateCacheSize="0"
		      xslCacheSize="2MB"
		      filteredItemsCacheSize="2MB"
		      allowDebug="false"
		      enablePreview="false"
		      enableWebEdit="false"
		      enableDebugger="false"
		      disableClientData="true" 
		/>
	</sites>
  </sitecore>
</configuration>